@model SocialInsurance.Models.UnemploymentInsurance

@{
    ViewBag.Title = "UnemploymentInsurance";
    Layout =null;
}
<style>
    .name {
        width:20%;
        text-align: center;
       
    }
    .nametemp {
        width:20%;
        text-align: center;
        padding-left:3px;
        padding-right:3px;
    }
</style>
@if (Model.status == "200")
{
    <div class="am-list" am-mode="flat form">
        <div class="am-list-item am-input-autoclear">
            <div class="am-list-body">未查到你要查询相关信息，请稍后再试</div>
        </div>
    </div>
}
else if (Model.status == "300")
{
    <div class="am-list" am-mode="flat form">
        <div class="am-list-item am-input-autoclear">
            <div class="am-list-body">网络错误,请稍后重试！</div>
        </div>
    </div>
}
else
{
    if (Model.status == "100")
    {
        <div id="resultmore">
            @if (Model.cpage == 1)
            {
                <input type="hidden" id="cpage" value='@Model.cpage' />
                <input type="hidden" id="pages" value='@Model.pages' />
                <input type="hidden" id="style" value="unemployment" />
                <div class="am-list-item">
                    <div class="name">结算期</div>
                    <div class="nametemp">缴费基数</div>
                    <div class="nametemp">缴费月数</div>
                    <div class="nametemp">缴费金额</div>
                    <div class="nametemp">到账标记</div>
                    @*<div class="name">缴费类别</div>*@
                </div>
            }
            @foreach (var item in Model.data)
            {
            <div class="am-list-item">
                <div class="name">@item.akf088</div>
                <div class="nametemp">@item.aif005</div>
                <div class="nametemp">@item.aif004</div>
                <div class="nametemp">@item.aif006</div>
                <div class="nametemp">@item.akf084</div>
                @*<div class="name">@item.akf081</div>*@
            </div>
            }
        </div>
    }
}   
       