@model SocialInsurance.Models.CompanyMonthInsurance

@{
    ViewBag.Title = "CompanyMonthInsurance";
    Layout = null;
}
<style>
    .name {
        width:20%;
        text-align: center;
    }
    .nametemp {
        width: 20%;
        text-align: center;
          padding-left:3px;
        padding-right:3px;
    
    }
</style>
@if (Model.status == "200")
{
    <div class="am-list-footer" style="padding-top:10px;padding:15px 10px 10px 15px;">
        未查到你要查询相关信息，请稍后再试
    </div>
}
else
{
    if (Model.status == "100")
    {
        <div id="resultmore">
            @if (Model.cpage == 1)
            {
                <input type="hidden" id="style" value="companymonth" />
                <div class="am-list-item">
                    <div class="name">结算期</div>
                    <div class="nametemp">账户本金</div>
                    <div class="nametemp">个人缴纳</div>
                    <div class="nametemp">月缴费基数</div>
                    @*<div class="nametemp">缴费月数</div>*@
                    <div class="name">到账标记</div>
                    @*<div class="name">缴费类别</div>*@
                </div>
            }
            @foreach (var item in Model.data)
            {
                <div class="am-list-item">
                    <div class="name">@item.akf088</div>
                    <div class="nametemp">@item.aif034</div>
                    <div class="nametemp">@item.aae082</div>
                    <div class="nametemp">@item.aae180</div>
                    <div class="nametemp">@item.aif004</div>
                    <div class="name">@item.akf084</div>
                    <div class="name">@item.akf081</div>
                </div>
            }
        </div>
    }
}

