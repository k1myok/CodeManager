
@{
    ViewBag.Title = "Default";
    Layout = "~/Views/Shared/_Layout1.cshtml";
}

<script>
    $(document).ready(function () {
        $('#HsplitLayout_Splitter').jqxSplitter({ width: '100%', height: '100%', panels: [{ size: 260 }] });

        $("#splitter-panel").ready(function () {
            $('#loader').show();
            $.ajax({
                url: '../Manage/GeneralFrameView',
                success: function (data) {
                    $('#splitter-panel').empty().html(data);
                },
                complete: function () {
                    $('#loader').hide();
                }
            });
        });
    });
</script>
<div id="HsplitLayout_Splitter">
    <div class="splitter-panel">
        @{
            Html.RenderAction("UserInfo", "SecurityManager");
        }
        <div class="HsplitLayout_LeftPanel" id="HsplitLayout_LeftPanel">
            <div class="SimpleWindow">
                <div class="Content">
                    <div class="SimpleListView">
                        <div class="Title" style="margin-left:40px;">
                            <img src="~/Images/metadata_manager.png" style="height:24px;" />
                            <span style="color:#ffffff">元数据管理</span>
                        </div>
                        <div class="Title" style="margin-left:40px;margin-top:10px;">
                            <img src="~/Images/metadata_manager.png" style="height:24px;" />
                            <span style="color:#ffffff">元数据管理</span>
                        </div>
                        <div class="Title" style="margin-left:40px;margin-top:10px;">
                            <img src="~/Images/metadata_manager.png" style="height:24px;" />
                            <span style="color:#ffffff">元数据管理</span>
                        </div>
                        <div class="Title" style="margin-left:40px;margin-top:10px;">
                            <img src="~/Images/metadata_manager.png" style="height:24px;" />
                            <span style="color:#ffffff">元数据管理</span>
                        </div>
                    </div>
                </div>         
            </div>
            
        </div>
    </div>
    <div class="splitter-panel" id="splitter-panel" style="height:100%;width:100%;">
        @*@RenderSection("RightPanel")*@
    </div>
</div>